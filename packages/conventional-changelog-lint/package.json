{
  "name": "@oriflame/conventional-changelog-lint-config",
  "version": "1.1.34",
  "description": "Conventional changelog lint config oriflame",
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "repository": {
    "url": "https://github.com/Oriflame/conventional-changelog-tools",
    "directory": "packages/conventional-changelog-lint"
  },
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "license": "MIT",
  "files": [
    "dts",
    "dts/**/*.d.ts",
    "dts/**/*.{d.ts,d.ts.map}",
    "lib",
    "lib/**/*.{js,map}",
    "src",
    "src/**/*.{ts,tsx,json}"
  ],
  "keywords": [
    "conventional-changelog",
    "oriflame",
    "preset",
    "commitlint"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@oriflame/conventional-changelog-types": "^1.0.37"
  },
  "peerDependencies": {
    "@commitlint/cli": ">=12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1"
  },
  "packemon": {
    "format": "lib",
    "platform": "node",
    "support": "current"
  },
  "engines": {
    "node": ">=14.16.0",
    "npm": ">=6.14.0 || >=7.0.0"
  }
}
